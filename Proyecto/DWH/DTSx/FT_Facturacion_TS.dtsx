    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{D7EDBEB6-4323-4520-80A9-5B4F9FDD8900}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select periodo, tipo_operacion, cod_empresa, cod_sucursal, cod_cliente, &#xA;&#x9;cod_servicio, cod_cuenta, sum(isnull(importe,0)) as importe&#xA;from (&#xA;&#x9;SELECT cast(convert(char(6),convert(smalldatetime,accountingdate),112) as varchar(10)) as periodo, &#xA;&#x9;&#x9;case when left(v.mainaccount,2) = '60' then 'VTA' else 'COS' end tipo_operacion,&#xA;&#x9;&#x9;cast(v.ledgername as varchar(30)) as cod_empresa, &#xA;&#x9;&#x9;cast(right(v.localcostcenter,6) as varchar(30)) as cod_sucursal, &#xA;&#x9;&#x9;cast(case when v.clientorganizationunit = '' then '00000' else v.clientorganizationunit end as varchar(30)) as cod_cliente,&#xA;&#x9;&#x9;cast(v.LocalSubserviceLine as varchar(30)) as cod_servicio,&#xA;&#x9;&#x9;cast(v.MainAccount as varchar(30)) as cod_cuenta,&#xA;&#x9;&#x9;convert(decimal(18,2),v.accountingcurrencyamount) as importe&#xA;&#x9;FROM dbo.D365_VOUCHER_TRANSACTIONS as v WITH(NOLOCK)&#xA;&#x9;WHERE 1=1 &#xA;&#x9;&#x9;and v.ledgername in('1181','1188','1185','1184','2012','2013','2015','2016','2018') &#xA;&#x9;&#x9;and v.postingtype &lt;&gt; 'TransferOpeningClosing'&#xA;&#x9;&#x9;and (left(v.mainaccount,2) = '60'&#xA;&#x9;&#x9;&#x9;or (left(v.mainaccount,2) = '65' and v.postinglayer in ('Current', 'Tax')) )&#xA;&#x9;&#x9;) vw&#xA;where periodo = '&quot;+  (DT_WSTR, 6) @[User::vIdMes] +&quot;' group  by periodo, tipo_operacion, cod_empresa, cod_sucursal, cod_cliente, &#xA;&#x9;cod_servicio, cod_cuenta&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="vQuery">
      <DTS:VariableValue
        DTS:DataType="8">select periodo, tipo_operacion, cod_empresa, cod_sucursal, cod_cliente, 
	cod_servicio, cod_cuenta, sum(isnull(importe,0)) as importe
from (
	SELECT cast(convert(char(6),convert(smalldatetime,accountingdate),112) as varchar(10)) as periodo, 
		case when left(v.mainaccount,2) = '60' then 'VTA' else 'COS' end tipo_operacion,
		cast(v.ledgername as varchar(30)) as cod_empresa, 
		cast(right(v.localcostcenter,6) as varchar(30)) as cod_sucursal, 
		cast(case when v.clientorganizationunit = '' then '00000' else v.clientorganizationunit end as varchar(30)) as cod_cliente,
		cast(v.LocalSubserviceLine as varchar(30)) as cod_servicio,
		cast(v.MainAccount as varchar(30)) as cod_cuenta,
		convert(decimal(18,2),v.accountingcurrencyamount) as importe
	FROM dbo.D365_VOUCHER_TRANSACTIONS as v WITH(NOLOCK)
	WHERE 1=1 
		and v.ledgername in('1181','1188','1185','1184','2012','2013','2015','2016','2018') 
		and v.postingtype &lt;&gt; 'TransferOpeningClosing'
		and (left(v.mainaccount,2) = '60'
			or (left(v.mainaccount,2) = '65' and v.postinglayer in ('Current', 'Tax')) )
		) vw
where periodo = '0' group  by periodo, tipo_operacion, cod_empresa, cod_sucursal, cod_cliente, 
	cod_servicio, cod_cuenta</DTS:VariableValue>
    </DTS:Variable>
