      DTS:ObjectName="vQueryManual">
      <DTS:VariableValue
        DTS:DataType="8">select PERIODO, COD_EMPRESA, COD_SUCURSAL, COD_CLIENTE, 
	COD_SERVICIO, COD_CUENTA, POSTING, sum(isnull(importe,0)) as IMPORTE
from (
	SELECT cast(convert(char(6),convert(smalldatetime,accountingdate),112) as varchar(10)) as periodo, 
		cast(v.ledgername as varchar(30)) as cod_empresa, 
		cast(right(v.localcostcenter,6) as varchar(30)) as cod_sucursal, 
		cast(case when v.clientorganizationunit = '' then '00000' else v.clientorganizationunit end as varchar(30)) as cod_cliente,
		cast(v.LocalSubserviceLine as varchar(30)) as cod_servicio,
		cast(v.MainAccount as varchar(30)) as cod_cuenta,
		cast(v.postinglayer as varchar(30)) as posting,
		convert(decimal(18,2),v.accountingcurrencyamount) as importe
	FROM dbo.D365_VOUCHER_TRANSACTIONS as v WITH(NOLOCK)
	WHERE 1=1 
		and v.ledgername in('1181','1188','1185','1184','2012','2013','2015','2016','2018') 
		and v.postingtype &lt;&gt; 'TransferOpeningClosing' 
		and left(v.mainaccount,2) &gt;= '60' ) vw
where periodo = '0' 
group  by periodo, cod_empresa, cod_sucursal, cod_cliente, 
	cod_servicio, cod_cuenta, posting</DTS:VariableValue>
