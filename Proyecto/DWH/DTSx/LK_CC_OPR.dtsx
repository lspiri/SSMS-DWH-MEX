                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT S.COD_SUCURSAL AS COD_CC,
	S.DESC_SUCURSAL AS DESC_CC,
	CASE WHEN R.COD_REGION = 'NA' THEN 'NA' ELSE R.DESC_REGION END AS REGION_OPR,
	CASE WHEN D.COD_DOP = 'NA' THEN 'NA' ELSE D.DESC_DOP END  AS DOP_OPR,
	CASE WHEN Z.COD_ZONA = 'NA' THEN 'NA' ELSE Z.DESC_ZONA END AS REGION_OPR_2,
	A.COD_AGRUPADOR AS AGRUPADOR
FROM DGEO_LK_SUCURSAL S,
	DGEO_LK_REGION R,
	DGEO_LK_DOP D,
	DGEN_LK_AGRUPADOR_CC A,
	DGEO_LK_ZONA Z
WHERE S.ID_REGION = R.ID_REGION
	AND R.ID_DOP = D.ID_DOP
	AND S.ID_AGRUPADOR = A.ID_AGRUPADOR
	AND S.ID_ZONA = Z.ID_ZONA
	AND S.ID_SUCURSAL &gt; 0</property>
